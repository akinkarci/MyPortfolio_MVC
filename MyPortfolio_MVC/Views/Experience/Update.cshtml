@model MyPortfolio_MVC.Models.TblExperience
@{
    ViewBag.Title = "Update";
    Layout = "~/Views/AdminLayout/Layout.cshtml";
}

<h2>Update</h2>

<form method="post">

    @Html.HiddenFor(x => x.ExperienceId)

    <label>Şirket Adı</label>
    @Html.TextBoxFor(x => x.CompanyName, new { @class = "form-control" })
    <br />

    <label>Rol</label>
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    <br />

    <label>Başlangıç Tarihi</label>
    @Html.TextBoxFor(x => x.StartDate, new { @class = "form-control" })
    <br />

    <label>Bitiş Tarihi</label>
    @Html.TextBoxFor(x => x.EndDate, new { @class = "form-control" })
    <br />

    <label>Görevi</label>
    @Html.TextBoxFor(x => x.Description, new { @class = "form-control" })
    <br />



    <button class="btn btn-outline-light">Kaydet</button>





</form>